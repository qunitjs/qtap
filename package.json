{
  "name": "qtap",
  "description": "Run unit tests in real browsers with TAP.",
  "version": "0.0.1",
  "homepage": "https://qunitjs.com",
  "license": "MIT",
  "author": "Timo Tijhof",
  "repository": {
    "type": "git",
    "url": "https://github.com/qunitjs/qtap.git"
  },
  "type": "module",
  "bin": {
    "qunit": "bin/qtap.js"
  },
  "main": "index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint-fix": "eslint --cache --fix .",
    "test": "node bin/qtap.js -v test/pass.html && npm run -s lint"
  },
  "dependencies": {
    "@tapjs/tap-finished": "0.0.3",
    "commander": "12.1.0",
    "kleur": "4.1.5",
    "tap-parser": "18.0.0",
    "which": "5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.13.0",
    "eslint-config-semistandard": "^17.0.0",
    "semistandard": "^17.0.0"
  }
}
